{
  "name": "unzip-iterable",
  "version": "0.1.3",
  "description": "Thin wrapper around yauzl",
  "main": "lib/index.js",
  "repository": "https://github.com/gordonmleigh/unzip-iterable",
  "author": "Gordon Leigh <gordon.leigh@futurematik.co.uk>",
  "license": "MIT",
  "keywords": [
    "zip",
    "archive",
    "unzip",
    "async",
    "iterable",
    "file"
